{
  "manifest_version": 3,
  "name": "NoAd",
  "version": "2.1.4",
  "description": "Ultimate Adblocker with YouTube support âš¡. Blocks more ads, popups, and trackers.",
  "permissions": [
    "storage",
    "declarativeNetRequest",
    "declarativeNetRequestFeedback",
    "tabs"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ads",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "content_scripts": [{
    "matches": ["*://*.youtube.com/*", "*://*.googleusercontent.com/youtube.com/1"],
    "js": ["contentScript.js"],
    "run_at": "document_start"
  }],
  "icons": {
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "popup/popup.html",
    "default_icon": {
      "128": "icons/128.png"
    }
  },
  "options_page": "options.html"
}